@model IEnumerable<Assign2Tennis.Models.Coach>

@{
    ViewData["Title"] = "Index";
}

<h2>Coach</h2>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Biography)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Email)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Biography)
                </td>
                <td>
                   <a asp-action="Edit" class="btn btn-default" asp-route-id="@item.Id">Edit</a>
                   <a asp-action="Delete" class="btn btn-default" asp-route-id="@item.Id">Delete</a>
                   <a asp-action="Details" class="btn btn-default" asp-route-id="@item.Id">Details</a>

                </td>
            </tr>
        }
    </tbody>
</table>
