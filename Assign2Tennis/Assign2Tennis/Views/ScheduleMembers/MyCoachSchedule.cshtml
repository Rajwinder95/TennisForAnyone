@model IEnumerable<Assign2Tennis.Models.ScheduleMembers>

@{
    ViewData["Title"] = "Index";
}

<h2>My Schedule Members</h2>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ScheduleId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Event)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Member)
            </th>
            <th> Coach </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ScheduleId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Event)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Member)
            </td>
            <td>
                @ViewBag.Coachname
            </td>
            <td>
                
                <a asp-action="Details" asp-route-id="@item.Id">Details</a>
                
            </td>
        </tr>
        }
    </tbody>
</table>
